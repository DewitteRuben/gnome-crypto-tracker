        <key name="show-indicator" type="b">
            <default>true</default>
        </key>
        <key name="coin" type="s">
            <default>"bitcoin"</default>
        </key>
        <key name="currency" type="s">
            <default>"eur"</default>
        </key>
        <key name="coin-icon-url" type="s">
            <default>""</default>
        </key>
    </schema>
</schemalist>
