<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema id="org.gnome.shell.extensions.cryptopricetracker" path="/org/gnome/shell/extensions/cryptopricetracker/">
        <!-- See also: https://developer.gnome.org/glib/stable/gvariant-format-strings.html -->
        <key name="coin" type="s">
            <default>"bitcoin"</default>
        </key>
        <key name="currency" type="s">
            <default>"eur"</default>
        </key>
        <key name="coin-icon-url" type="s">
            <default>"https://assets.coingecko.com/coins/images/1/small/bitcoin.png"</default>
        </key>
        <key name="price-change-range" type="s">
            <default>"24h"</default>
        </key>
    </schema>
</schemalist>
